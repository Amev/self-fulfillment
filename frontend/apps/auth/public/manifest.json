{
  "manifest_version": 3,
  "name": "Anti procrastination",
  "description": "Control the time you spend on un-productive/non-productive sites",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "optional_host_permissions": [
    "https://*.facebook.com/",
    "https://*.youtube.com/"
  ],
  "background": {
    "service_worker": "background.es.js"
  }
}
